{
    "name": "hgabka/doctrine-translatable-bundle",
    "type": "symfony-bundle",
    "description": "Integrate the doctrine-translatable extension in Symfony2",
    "keywords": [
        "doctrine2",
        "extensions",
        "translatable",
        "i18n",
        "symfony2",
        "bundle"
    ],
    "homepage": "http://hgabka.eu",
    "authors": [{
        "name": "Gabe"
    }],
    "require": {
        "doctrine/collections": "^1.6",
        "doctrine/common": "^3.0",
        "doctrine/inflector": "^2.0",
        "doctrine/persistence": "^2.0",
        "hgabka/doctrine-translatable": "^6.0",
        "symfony/framework-bundle": "6.0.*",
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.1"
    },
    "autoload": {
        "psr-4": { "Hgabka\\Doctrine\\TranslatableBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
