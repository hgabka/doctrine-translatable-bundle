{
    "name": "hgabka/doctrine-translatable-bundle",
    "type": "symfony-bundle",
    "description": "Integrate the doctrine-translatable extension in Symfony2",
    "keywords": [
        "doctrine2",
        "extensions",
        "translatable",
        "i18n",
        "symfony2",
        "bundle"
    ],
    "homepage": "http://hgabka.eu",
    "authors": [{
        "name": "Gabe"
    }],
    "require": {
        "doctrine/doctrine-bundle": "^1.2|^2.0",
        "hgabka/doctrine-translatable": "^1.0|^2.0",
        "symfony/config": "^2.7|^3.0|^4.0|^5.0",
        "symfony/dependency-injection": "^2.7|^3.0|^4.0|^5.0",
        "symfony/event-dispatcher": "^2.7|^3.0|^4.0|^5.0",
        "symfony/http-foundation": "^2.7|^3.0|^4.0|^5.0",
        "symfony/http-kernel": "^2.7|^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.1"
    },
    "autoload": {
        "psr-4": { "Hgabka\\Doctrine\\TranslatableBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
